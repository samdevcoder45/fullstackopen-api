{
  "name": "fulstackopen-api",
  "version": "0.0.1",
  "description": "notesapp api",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production ts-node build/index.js",
    "build": "npx tsc",
    "build:ui": "rm -rf dist && cd ../fullstack-open && npm run build && cp -r dist ../fulstackopen-api",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push",
    "dev": "cross-env NODE_ENV=development nodemon build/index.js",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test node --test"
  },
  "author": "samdevcoder45",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.4.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.13.0",
    "cross-env": "^7.0.3",
    "globals": "^15.3.0",
    "nodemon": "^3.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.4.1"
  }
}
